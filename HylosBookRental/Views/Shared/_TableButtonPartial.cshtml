@model HylosBookRental.Models.IndividualButtonPartial
@using HylosBookRental.Models





<td style="width:400px;">
    <div class="btn-group" role="group">
        @Html.Partial("_IndividualButtonPartial", new IndividualButtonPartial
        {
        Action = "Edit",
        ButtonType = "btn-primary",
        Gylph = "pencil",
        Text = "Edit Button",
        GenreId = Model.GenreId,
        BookId = Model.BookId,
        CustomerId = Model.CustomerId,
        MembershipTypeId = Model.MembershipTypeId,
        UserId = Model.UserId
        })

        @Html.Partial("_IndividualButtonPartial", new IndividualButtonPartial
   {
       Action = "Details",
       ButtonType = "btn-success",
       Gylph = "list",
       Text = "Details Button",
       GenreId = Model.GenreId,
       BookId = Model.BookId,
       CustomerId = Model.CustomerId,
       MembershipTypeId = Model.MembershipTypeId,
       UserId = Model.UserId
   })

        @Html.Partial("_IndividualButtonPartial", new IndividualButtonPartial
   {
       Action = "Delete",
       ButtonType = "btn-danger",
       Gylph = "trash",
       Text = "Delete Button",
       GenreId = Model.GenreId,
       BookId = Model.BookId,
       CustomerId = Model.CustomerId,
       MembershipTypeId = Model.MembershipTypeId,
       UserId = Model.UserId
   })
    </div>
</td>



